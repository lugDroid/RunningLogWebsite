@model  Athlete

@{
    ViewData["Title"] = "Athlete Data";
}

<div class="panel panel-default">
    <h4 class="panel-heading display-4 text-info">@ViewData["Title"]</h4>

    <div class="card" style="width: 30rem">
        <ul class="list-group-flush">
            <li class="list-group-item">First Name: @Model.FirstName</li>
            <li class="list-group-item">Last Name: @Model.LastName</li>
            <li class="list-group-item">City: @Model.City</li>
            <li class="list-group-item">State: @Model.State</li>
            <li class="list-group-item">Country: @Model.Country</li>
            <li class="list-group-item">Created At: @Model.CreatedAt</li>
            <li class="list-group-item">Updated At: @Model.UpdatedAt</li>
        </ul>
    </div>

</div> 
